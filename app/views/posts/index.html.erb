  </head>
  <body>
    <div id="global-container">
      <div id="container">
        <div class="mobile-menu__cover"></div>
        <div class="nav-trigger"></div>
        <header class="header">
          <div class="header__inner appear up">
            <div class="logo item">
              <span class="logo__stay">RE</span>
              <span class="logo__world">ISE</span>
            </div>
            <nav class="header__nav">
              <ul class="header__ul">
                <%= link_to "投稿", new_post_path, class: "header__li item" %>
                <li class="header__li item"><a href="#">投稿</a></li>
                <li class="header__li item"><a href="#">Help</a></li>
                <li class="header__li item"><a href="#">Cart</a></li>
                <li class="header__li item">
                  <a class="letter-spacing" href="#">My account</a>
                </li>
              </ul>
            </nav>
            <button class="mobile-menu__btn">
              <span></span>
              <span></span>
              <span></span>
            </button>
          </div>
        </header>
                  <div class="houses__inner">
                  <div class="houses__item">
                    <div class="cover-slide hover-darken">
                      <div
                        class="bg-img-zoom img-bg50"
                        style="background-image: url(assets/image-55.jpg)"
                      ></div>
                    </div>
                    <p class="houses__title">MENS</p>
                  </div>
                  <div class="houses__item">
                    <div class="cover-slide hover-darken">
                      <div
                        class="bg-img-zoom img-bg50"
